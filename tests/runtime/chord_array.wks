# @desc: Chord array syntax [xyz] and implicit arrays

# @test: "x"
# @expect: switching to 10

# @test: "y"
# @expect: switching to 11

# @test: "z"
# @expect: switching to 12

... "Switch %(index+1)" +write %{{switching to %(index)}}
[xyz] "Switch %(index+1)" +write %{{switching to %(index)}}
C-M-... "Switch %(index+1)" +write %{{switching to %(index)}}
