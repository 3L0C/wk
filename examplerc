e 'Echo keys' :before ${{echo first}} :after ${{echo last}}
{
    a 'A prefix!' :inherit
    {
        [abcd] '%(key) chord!' ${{echo %(key)}}
    }
    [\: C-S-RET ãƒ†rstg] 'Tag %(index+1)' ${{
        echo "%(key)"
    }}
}
t 'Tag & follow' :before ${{dwmc viewex %(index)}}
{
    [arstneio] 'Tag %(index+1)' :keep ${{
        notify-send "dwmc" "Viewing tag %(index+1)"
    }}
}
